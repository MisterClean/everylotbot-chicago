# Cook County data token
CHICAGO_DATA_PORTAL_TOKEN=your_token_here

# Google Street View & Geocoding
GOOGLE_API_KEY=your_google_api_key

# Bluesky
BLUESKY_IDENTIFIER=your.bsky.social
BLUESKY_PASSWORD=your_password

# Twitter (optional)
TWITTER_CONSUMER_KEY=your_consumer_key
TWITTER_CONSUMER_SECRET=your_consumer_secret
TWITTER_ACCESS_TOKEN=your_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_access_token_secret

# Toggles
ENABLE_TWITTER=false
ENABLE_BLUESKY=true

# Starting PIN10 (optional)
START_PIN10=

# Bot Configuration
SEARCH_FORMAT="{address}, {city} {state}"
PRINT_FORMAT="{address}"

# Camera Settings
STREETVIEW_PITCH=-11.55
STREETVIEW_ZOOM=1

# Database
DATABASE_PATH=cook_county_lots.db
